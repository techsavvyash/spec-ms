{
    "ingestion_type": "dataset",
    "dataset_name": <DatasetName>,
    "input": {
        "type": "object",
        "properties": {
            "dataset_name": {
                "type": "string"
            },
            "dimensions": {
                "type": "object",
                "properties": {
                    "table":  <DimensionTable>,
                    "column": <DimensionCol>,
                    "merge_on_col": <MergeOnCol>
                }
            },
            "dataset": {
                "type": "object",
                "properties": {
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": <DatasetObject>,
                            "required":  <DatasetList>
                        }
                    },
                    "group_by": <GroupByCol>,
                    "aggregate": {
                        "type": "object",
                        "properties": {
                            "function": <AggFunction>,
                            "target_table": <TargetTable>,
                            "numerator_col": <NumeratorCol>,
                            "denominator_col": <DenominatorCol>,
                            "update_cols":  <UpdateCol>,
                            "columns": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "column":  <AggCol>
                                    }
                                }
                            },
                            "filters": {
                                "type": "object",
                                "properties": {
                                    "filter_col": <FilterCol>,
                                    "filter_type": <FilterType>,
                                    "filter": <Filter>
                                }
                            },
                            "required": [
                                "function",
                                "target_table",
                                "update_cols",
                                "columns",
                                "filters"
                            ]
                        }
                    },
                    "required": [
                        "items"
                    ]
                }
            },
            "required": [
                "dataset_name",
                "dataset"
            ]
        }
    }
}
